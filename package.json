{
  "name": "refrigerator-recipes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:staging": "NODE_ENV=staging next build",
    "build:production": "NODE_ENV=production next build",
    "start:staging": "NODE_ENV=staging next start",
    "start:production": "NODE_ENV=production next start",
    "analyze": "ANALYZE=true next build",
    "security-audit": "npm audit && npm audit fix",
    "type-check": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .next node_modules/.cache",
    "clean:all": "rimraf .next node_modules/.cache node_modules package-lock.json && npm install",
    "dev:clean": "npm run clean && npm run dev",
    "build:clean": "npm run clean && npm run build",
    "reset": "npm run clean:all && npm run dev"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.0.0",
    "@sentry/react": "^8.0.0",
    "@tanstack/react-query": "^5.85.3",
    "@tanstack/react-query-devtools": "^5.85.3",
    "clsx": "^2.1.1",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.12",
    "mixpanel-browser": "^2.49.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-firebase-hooks": "^5.1.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
